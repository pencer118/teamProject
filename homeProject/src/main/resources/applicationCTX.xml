<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- MvcBoardList 클래스의 bean을 만든다. -->
	<bean id="loginList" class="kr.koreait.vo.LoginList"/> <!-- 기본 생성자로 만든 bean -->
	
	<!-- noticeVO 클래스의 bean을 만든다. -->
	<bean id="noticeVO" class="kr.koreait.vo.NoticeVO"/>
	<!-- MvcBoardList 클래스의 bean을 만든다. -->
	<bean id="noticeList" class="kr.koreait.vo.NoticeList"/> <!-- 기본 생성자로 만든 bean -->
	
	<bean id="goodsList" class="kr.koreait.vo.GoodsList"/>
	<!-- QAVO 클래스의 bean을 만든다. -->
	   <bean id="qaVO" class="kr.koreait.vo.QAVO"/>
	   <!-- QAList 클래스의 bean을 만든다. -->
	   <bean id="qaList" class="kr.koreait.vo.QAList"/>
	   
	   <!-- reviewVO 클래스의 bean을 만든다. -->
	<bean id="reviewVO" class="kr.koreait.vo.ReviewVO"/>
	<!-- MvcBoardList 클래스의 bean을 만든다. -->
	<bean id="reviewList" class="kr.koreait.vo.ReviewList"/> 
</beans>
